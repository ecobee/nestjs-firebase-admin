{
  "name": "@ecobee/nestjs-firebase-admin",
  "version": "3.0.0",
  "description": "Firebase Admin module for Nest framework",
  "authors": [
    {
      "name": "Nonpawit Teerachetmongkol",
      "email": "nonpawit.tee@aginix.tech"
    },
    {
      "name": "Michael Musso",
