  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aginix/nestjs-firebase-admin.git"
  },
  "scripts": {
    "test": "jest --config jest.json",
    "test:dev": "jest --config jest.json --watch",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "build": "rimraf dist && npm run build:src && npm test",
    "build:src": "tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.4.3 || ^10.3.3",
    "@nestjs/core": "^8.0.0 || ^9.4.3 || ^10.3.3"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0 || ^9.4.3 || ^10.3.3",
    "@nestjs/core": "^8.0.0 || ^9.4.3 || ^10.3.3",
    "firebase-admin": "^11.11.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^14.6.0",
    "dotenv": "^8.1.0",
    "husky": "^2.6.0",
    "jest": "^29.7.0",
    "prettier": "^2.1.0",
    "pretty-quick": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^4.0.0",
    "tslint": "5.16.0",
    "typescript": "~4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
